{
  "name": "@shelf/babel-config",
  "version": "0.0.3",
  "description": "babel-config",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "https://shelf.io"
  },
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "backend.js",
    "frontend-component.js"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "@babel/preset-env": "7.13.9",
    "@babel/preset-typescript": "7.13.0"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.28.0",
    "@shelf/prettier-config": "1.0.0",
    "eslint": "7.21.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
